            p.pz = Val(txtVz.Text)
            p.a = Val(tta.Text)
        Case 2 'repartida

            p.px = Val(txtVx.Text)
            p.py = Val(txtVy.Text)
            p.pz = Val(txtVz.Text)

        Case 3 'repartida parcial

            p.px = Val(txtVx.Text)
            p.py = Val(txtVy.Text)
            p.pz = Val(txtVz.Text)
            p.a = Val(tta.Text)
            p.b = Val(ttb.Text)
        Case 4

            p.px = Val(txtVx.Text)
            p.py = Val(txtVy.Text)
            p.a = Val(tta.Text)
            p.b = Val(ttb.Text)

        Case 5

            p.px = Val(txtVx.Text)
            p.py = Val(txtVy.Text)
            p.pz = Val(txtVz.Text)

        Case 6

            p.px = Val(txtVx.Text)
            p.py = Val(txtVy.Text)
            p.pz = Val(txtVz.Text)

    End Select
    'End If
    'Next
    dibujarbarracargada(a)

End Sub

